aws_machines=( melbourne sydney california bahrain mumbai saopaulo stockholm ireland tokyo virginia )
other_machines=( guam )

source_aws_env () {
  export AWS_PROFILE="${1}"
  echo "AWS_PROFILE set to ${AWS_PROFILE}"
}

source_docker_machine_env () {
  eval "$(docker-machine env "${1}")"
  echo Active docker machine: "$(docker-machine active)"
}

source_docker_compose_env () {
  local source_file=".env.${1}"
  source ${source_file}
  echo "Sourcing ${source_file}"
}
